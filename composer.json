{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.2.1",
        "vespakoen/menu": "dev-master",
        "intervention/image": "dev-master",
        "anlutro/curl": "dev-master",
        "doctrine/dbal": "*",
        "components/jquery": "1.11.0",
        "components/jqueryui": "1.10.4",
        "twitter/bootstrap": "3.1.1",
        "jasny/bootstrap": "v3.1.3",
        "fortawesome/font-awesome": "v4.0.3",
        "laracasts/Presenter": "0.1.4",
        "symfony/http-foundation": "2.5"
    },
    "require-dev": {
        "way/generators": "dev-master",
        "codeception/codeception": "*",
        "phpspec/phpspec": "~2.0",
        "behat/behat": "~2.5.1",
        "behat/mink": "~1.5.0",
        "behat/mink-extension": "~1.2.0",
        "behat/mink-goutte-driver": "~1.0.9",
        "behat/mink-selenium2-driver": "~1.1.1",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/library",
            "app/models",
            "app/presenters",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "vendor/nukacode/coreOld/src/database/migrations",
            "vendor/nukacode/coreOld/src/database/seeds"
        ],
        "files": ["app/start/helpers.php"],
        "psr-4": {
            "NukaCode\\Core\\": "vendor/nukacode/core/src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "vendor/components"
    },
    "minimum-stability": "dev"
}
